  body
    header.head-login
      .header-inner
        .container
          ul 
            li
              a(href='request.html') 
                i.material-icons touch_app
                | Requests
              a(href='notifications.html')
                i.material-icons notifications 
                | Notifications
              a(href='')
                i.material-icons add_box 
                | New Item
      .login-account       
        .container
          .col-md-2.login-img
            img.img-responsive(src='/images/logo_blue.svg', alt='Hangurs')
          .col-md-10
            a(href='account-details.html')
             | Account Details
            a(href='wallet.html') 
             | Wallet
            a(href='privacy.html') 
             | Privacy
            a(href='') 
              | Help
             a(href='') 
              | About
            a(href='index.html')
                | Log Out
            
      
      .wallet-details
        h2.inner-heading My Wallet
        .container
          .row.wallet-main
            .col-md-5.col-md-offset-1
              h4 
                span $359.2 
                | in my Wallet
            .col-md-5.col-md-offset-1
              a(href='#') withdraw
              i.fa.fa-chevron-right(aria-hidden='true')
          .row.wallet-inner
            table.mdl-data-table.mdl-js-data-table.mdl-shadow--2dp
              tbody
                tr
                  td.mdl-data-table__cell--non-numeric
                    img.img-responsive(src='/images/Card.png', alt='Hangurs')
                  td.mdl-data-table__cell--non-numeric 
                    h4 Credit carrd 2
                    span **********2641
                  td.mdl-data-table__cell--non-numeric 
                    i.fa.fa-chevron-right(aria-hidden='true')
                tr
                  td.mdl-data-table__cell--non-numeric
                    img.img-responsive(src='/images/Paypal logo.png', alt='Hangurs')
                  td.mdl-data-table__cell--non-numeric 
                    h4 Credit carrd 2
                    span **********2641
                  td.mdl-data-table__cell--non-numeric 
                    i.fa.fa-chevron-right(aria-hidden='true')
          .row.wallet-tabs
            h2 
              i.material-icons access_time 
              | Transaction 
              span.green History
              i.material-icons.top-icon vertical_align_top
            .mdl-tabs.mdl-js-tabs.mdl-js-ripple-effect
